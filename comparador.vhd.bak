library ieee;
use ieee.std_logic_1164.all;
use ieee.numeric_std.all;

entity comparador is
port (
	A, B : in std_logic_vector (3 downto 0);
	salida : out std_logic
);
end comparador;

architecture LEQ of comparador is

begin 
if (A<B)
	salida <= '1';
else 
	salida = '0';

end

end LEQ;
