LIBRARY IEEE;
USE IEEE.STD_LOGIC_1164.ALL;
USE IEEE.NUMERIC_STD.ALL;

ENTITY MULTIPLEXAB IS 
PORT(
	D0 : IN STD_LOGIC;
	OUTPUT : OUT STD_LOGIC
);
END MULTIPLEXAB;

ARCHITECTURE BEHAVIORAL OF MULTIPLEXAB IS
 SIGNAL SEL: STD_LOGIC:='0';
  GEN_CLK: PROCESS(D0)
  BEGIN
  IF(RISING_EDGE(D0))THEN
   SEL<=NOT(SEL);
	END IF;
	END PROCESS;
OUTPUT<=SEL;
END BEHAVIORAL;